{
    "name": "beowulf",
    "server": {
        "port": 8443,
        "enableForking": false,
        "forks": 1,
        "recoveryMax": 3,
        "logRequest": true,
        "allowPreFlightRequest": true,
        "allowedMethods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "OPTIONS"
        ],
        "allowOrigin": "*",
        "allowedHeaders": [
            "Content-Type",
            "Accept",
            "Authorization"
        ],
        "enableTestEndpoint": true
    },
    "security": {
        "useTls": false,
        "pfx": "",
        "ca": [],
        "key": "",
        "requestCert": false,
        "rejectUnauthorized": false
    },
    "logging": {
        "src": false,
        "level": "trace",
        "useStdOut": true,
        "path": "./logs"
    }
}